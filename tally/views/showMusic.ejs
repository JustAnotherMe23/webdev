<!DOCTYPE html>
<html>
    <head>
        <title>TRadio</title>
        <link href="https://fonts.googleapis.com/css?family=Cinzel:900|Lato:300" rel="stylesheet">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" href="/tally/views/styles.css"></link>
    </head>
    <body>
        
        <div id="navbar">
            <div id="top_line">
                <div id="logo">Tally Radio</div>
                <div id="vote">
                    <form>
                        <input type="text" class="text_input" id="vote_input"></input>
                        <input type="submit" class="submit" id="vote_button"></input>
                    </form>
                </div>
            </div>
            <div id="nav_options">
                <div class="nav_item">Playing ...</div>
                <div class="nav_item">Search</div>
                <div class="nav_item">About</div>
            </div>
        </div>
        
        <div id="container">
            <div id="spacer"></div>
            <iframe width="100%" height="166" scrolling="no" frameborder="no" 
            src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/293&amp;&sharing=false"></iframe>
            <div id="chat_box">
                <form onsubmit="return false" autocomplete="off">
                    <input type="text" id="chat_input"></input>
                </form>
                <div id="chat_comments"></div>
            </div>
        </div>
        
        <script type="text/javascript" src="/tally/views/showMusic.js"></script>
    </body>
</html>


